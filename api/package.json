{
  "name": "zyvo-api",
  "version": "1.0.0",
  "description": "A secure Node.js API server with Express.js",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon app.js",
    "test": "jest --config=jest.config.new.js",
    "test:watch": "jest --config=jest.config.new.js --watch",
    "test:coverage": "jest --config=jest.config.new.js --coverage",
    "test:unit": "jest --config=jest.config.new.js --selectProjects=unit",
    "test:unit:controllers": "jest --config=jest.config.new.js --selectProjects=unit tests/unit/controllers",
    "test:unit:models": "jest --config=jest.config.new.js --selectProjects=unit tests/unit/models",
    "test:unit:utils": "jest --config=jest.config.new.js --selectProjects=unit tests/unit/utils",
    "test:unit:coverage": "jest --config=jest.config.new.js --selectProjects=unit --coverage",
    "test:integration": "jest --config=jest.config.new.js --selectProjects=integration",
    "test:integration:api": "jest --config=jest.config.new.js --selectProjects=integration tests/integration/api",
    "test:integration:database": "jest --config=jest.config.new.js --selectProjects=integration tests/integration/database",
    "test:performance": "jest --config=jest.config.new.js --selectProjects=performance",
    "test:e2e": "jest --config=jest.config.new.js --selectProjects=e2e",
    "test:controller": "jest --config=jest.config.new.js --selectProjects=unit tests/unit/controllers",
    "test:favorites": "jest --config=jest.config.new.js tests/**/favorite*.test.js",
    "test:cart": "jest --config=jest.config.new.js tests/**/cart*.test.js",
    "test:order": "jest --config=jest.config.new.js tests/**/order*.test.js",
    "test:reviews": "jest --config=jest.config.new.js tests/**/productReview*.test.js",
    "test:old": "jest --config=jest.config.js",
    "test:100-pass": "jest --config=jest.config.100-pass.js",
    "test:100-pass:coverage": "jest --config=jest.config.100-pass.js --coverage",
    "docs:generate": "node scripts/generateDocs.js",
    "docs:serve": "npx @redocly/cli preview-docs docs/openapi.yaml",
    "docs:full": "npm run docs:generate",
    "docs:watch": "node scripts/generateDocs.js --watch",
    "docs:setup": "node scripts/setup-docs.js",
    "seed": "node seeders/seeder.js",
    "seed:users": "node seeders/seeder.js seed users",
    "seed:products": "node seeders/seeder.js seed products",
    "seed:orders": "node seeders/seeder.js seed orders",
    "seed:blogPosts": "node seeders/seeder.js seed blogPosts",
    "seed:favorites": "node seeders/seeder.js seed favorites",
    "seed:all": "node seeders/seeder.js seed all",
    "clean:users": "node seeders/seeder.js clean users",
    "clean:products": "node seeders/seeder.js clean products",
    "clean:orders": "node seeders/seeder.js clean orders",
    "clean:blogPosts": "node seeders/seeder.js clean blogPosts",
    "clean:favorites": "node seeders/seeder.js clean favorites",
    "clean:all": "node seeders/seeder.js clean all",
    "db:status": "node seeders/seeder.js status",
    "db:reset": "npm run clean:all && npm run seed:all",
    "setup:users": "node scripts/setup-user-management.js",
    "setup:auth": "node scripts/setup-auth.js",
    "setup:all": "npm run setup:auth && npm run setup:users"
  },
  "keywords": [
    "express",
    "api",
    "node",
    "security"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@faker-js/faker": "^8.0.2",
    "bcrypt": "^5.1.0",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "csurf": "^1.11.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.8.1",
    "express-session": "^1.17.3",
    "express-validator": "^7.2.1",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.4.0",
    "morgan": "^1.10.0",
    "node-fetch": "^2.7.0",
    "nodemailer": "^6.10.1",
    "razorpay": "^2.9.6",
    "slugify": "^1.6.6",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "unsplash-js": "^7.0.19",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@redocly/cli": "^1.34.4",
    "jest": "^29.6.1",
    "mongodb-memory-server": "^10.1.4",
    "nodemon": "^3.0.1",
    "redoc-cli": "^0.13.21",
    "supertest": "^6.3.4",
    "yamljs": "^0.3.0"
  }
}
